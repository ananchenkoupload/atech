.template-product {
    .two-colum-image-wrap {
        max-height: 530px;
    }
    // .main-content .shopify-section {
    //     padding: 45px 0;
    // }
    .ext-wrap {
        display: none;
        position: fixed;
        z-index: 99999;
    }
    .m-close {
        position: absolute;
        top: 0;
        right: 0;
        display: block;
        overflow: visible;
        width: 35px;
        height: 35px;
        margin: 0;
        padding: 0;
        cursor: pointer;
        transition: color 0.2s;
        text-decoration: none;
        color: #95979c;
        border: 0;
        outline: 0;
        background: transparent;
        z-index: 1000;
        color: #fff;
        &:before {
            font-family: Arial, "Helvetica CY", "Nimbus Sans L", sans-serif !important;
            font-size: 25px;
            line-height: 35px;
            position: absolute;
            top: 0;
            left: 0;
            display: block;
            width: 35px;
            content: "\00d7";
            text-align: center;
        }
    }
    .m-common-wrap {
        padding: 10px 10px 0;
        position: fixed;
        z-index: 10000;
        top: 0;
        right: 0;
        bottom: 0;
        left: 0;
        overflow: auto;
        text-align: center;
        -webkit-overflow-scrolling: touch;
        background: rgba(43, 46, 56, 0.9);
        display: flex;
        justify-content: center;
        align-items: center;
        .m-common-inner {
            padding: 0;
            max-width: 940px;
            width: 100%;
            margin-bottom: 10px;
            // padding: 35px;
            transform: translate3d(0, 0, 0);
            color: #2b2e38;
            background: #fff;
            display: flex;
            flex-wrap: wrap;
            position: relative;
            overflow-y: auto !important;
            max-height: calc(100vh - 70px);
        }
        .m-common-first {
            width: calc(56.25% - 20px);
            margin-left: calc(20px / 2);
            margin-right: calc(20px / 2);
            box-sizing: border-box;
            text-align: left;
            padding: 30px 20px 5px 40px;
        }
        .m-common-sec {
            width: calc(43.75% - 20px);
            margin-left: auto;
            box-sizing: border-box;
            text-align: left;
            background-position: center;
            background-repeat: no-repeat;
            background-size: cover;
            img {
                display: none;
            }
        }
        .m-title-bottom {
            width: 100%;
            text-align: center;
            display: block;
            margin-bottom: 18px;
        }
        .m-flex {
            display: flex;
            flex-wrap: wrap;
            align-items: center;
            img {
                max-width: 130px;
            }
            .m-inner-text {
                width: calc(100% - 150px);
                margin-left: auto;
            }
        }
        .m-prod-description {
            margin: 25px 0;
        }
        .select,
        .purchase-details {
            display: none;
        }
        .swatch {
            margin-bottom: 0;
        }
        .swatch .swatch-element {
            width: 100%;
            background-color: #955640;
            color: #fff !important;
            border: medium none !important;
            padding: 0;
            box-shadow: none !important;
            border-radius: 0px !important;
            min-width: 40px;
            min-height: 25px;
            margin: 0 0 18px;
            label {
                width: 100%;
                font-size: 18px;
                height: auto !important;
                padding: 10px !important;
                text-transform: uppercase;
                min-width: 45px !important;
                line-height: 23.5px;
                font-weight: 300;
                cursor: pointer;
                background: none;
                border: none;
                text-align: center;
            }
        }
        ul {
            li {
                list-style: inside;
                display: list-item;
            }
        }
    }
    .product-single .m-ext-single {
        margin: -30px 0 23px;
        position: relative;
        z-index: 3;
        .swatch input {
            display: none;
        }
        .m-ext-single-tittle {
            font-size: 14px;
            line-height: 150%;
            color: #212121;
        }
        .select,
        .purchase-details {
            display: none;
        }
        .swatch {
            max-width: 460px;
            label {
                font-size: 12px;
                line-height: 150%;
                letter-spacing: 0.2px;
                background: none;
                border: none;
                color: #212121;
                border-radius: 0;
                &:hover {
                    text-decoration: underline;
                }
            }
            .swatch-element {
                margin: 0px 16px 0 0;
            }
        }
    }
}

