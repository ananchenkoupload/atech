}
.accodian-rt-img {
    width: 100%;
    background-size: cover;
    background-repeat: no-repeat;
}
.accodian-rt-img img {
    width: 100%;
    display: block;
}
.list-accodian .accodian-sec > li {
    border-top: 1px solid #7d7d7d;
    padding: 10px 30px 10px 0;
}
.list-accodian .accodian-sec > li p {
    font-size: 12px;
    line-height: 20px;
}
.list-accodian .accodian-sec > li h4 {
    margin-bottom: 0px;
    font-size: 16px;
    font-weight: 300;
    text-transform: uppercase;
    font-family: "GT America";
}
.acc__panel {
    display: none;
}

.accodian-content .accodian-sec .acc__panel, .accodian-content .accodian-with-list li {
    ul {
        margin-left: 30px;
        li {
            padding: 4px 0;
            position: relative;
            font-size: 14px;
            &:before {
                content: "";
                width: 6px;
                height: 6px;
                background: #ba9163;
                position: absolute;
                left: -18px;
                top: 12px;
                border-radius: 100%;
            }
        }
    }
}

.acc__title::after {
    position: absolute;
    right: -30px;
    content: "";
    background-image: url(../assets/CLOSE_PLUS_BLACK_N.svg);
    background-repeat: no-repeat;
    top: 1px;
    transition: all 0.2s ease-in-out;
    border: 1px solid #2f2f2f;
    width: 20px;
    height: 20px;
    text-align: center;
    line-height: 16px;
    border-radius: 50%;
    font-size: 26px;
    cursor: pointer;
    background-size: 67%;
    background-position: center center;
}
.acc__title:hover {
    &:after {
        opacity: 0.5;
    }
}
.acc__title.active::after {
    -webkit-transform: rotate(180deg);
    transform: rotate(180deg);
    -webkit-transition: all 0.2s ease-in-out;
    transition: all 0.2s ease-in-out;
    border-radius: 0;
    background-size: 18px !important;
    background-image: url(../assets/CLOSE_PLUS_BLACK_N-a.svg);
}
.acc__title {
    position: relative;
}
.accodian-content .accodian-with-list {
    display: none;
}
.accodian-sec {
    display: none;
}
.accodian-content .accodian-sec {
    display: block;
}
.accodian-content .accodian-sec .acc__title::after {
    border: none;
    right: -30px;
    // background-position: initial;
    background-size: contain;
}
.accodian-content .accodian-sec li .acc__title h4 {
