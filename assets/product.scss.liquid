    max-width: calc(1170px + 60px);
    padding: 0 30px;

    @media (max-width: 768px) {
        padding: 0;
    }
}
.product-description {
    @media (max-width: 768px) {
        padding: 0 30px;
    }
    @media (max-width: 400px) {
        padding: 0 20px;
    }
}
