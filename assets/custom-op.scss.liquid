/* template 404 */
.template-404 .main-content {
	padding-top: 50px;
	padding-bottom: 50px;
	.h3 {
		font-size: 22px;
	}
	.empty-page-content {
		padding: 0px;
	}
	.desc {
		font-size: 16px;
		line-height: 24px;
		color: #212121;
		padding-top: 20px;
		padding-bottom: 60px;
		margin-bottom: 0px;
	}
	.info {
		letter-spacing: 0px;
		font-size: 16px;
		line-height: 140%;
