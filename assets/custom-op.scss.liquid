/* template  Klaviyo */
div[formtype="POPUP"] > div {
	transform: scale(1) !important;
	@media (min-width: 750px) and (max-width: 1024px) {
		width: 90% !important;
	}
	@media (min-width: 320px) and (max-width: 550px) {
		width: 95% !important;
	}
}
div[formtype="POPUP"] div[formtype="POPUP"] {
	margin-left: 30px;
	margin-right: 30px;
	@media (min-width: 320px) and (max-width: 550px) {
		margin-left: 10px;
		margin-right: 10px;
	}
	> button {
		outline: none !important;
		right: 40px !important;
		@media (min-width: 320px) and (max-width: 550px) {
			right: 20px !important;
		}
		&:after {
			content: " ";
			background: url("{{ 'close.png' | asset_url }}") no-repeat center center;
			width: 30px;
			height: 30px;
			display: inline-block;
			position: absolute;
			left: 0px;
			top: 0px;
			pointer-events: none;
		}
		img {
			//display: none;
			width: 30px;
			height: 30px;
			opacity: 0.1;
		}
	}
	form {
		@media (min-width: 1200px) {
			width: 1024px !important;
		}
		@media (min-width: 750px) and (max-width: 1199px) {
			max-width: 1024px !important;
			width: 100% !important;
		}
		@media (min-width: 320px) and (max-width: 749px) {
			max-width: 440px !important;
			width: 100% !important;
		}
		> div {
			@media (min-width: 750px) {
				max-width: 820px;
				margin: 0 auto;
				flex-direction: row !important;
				position: relative;
			}
			@media (min-width: 320px) and (max-width: 749px) {
				max-width: 440px !important;
				flex-direction: column;
			}
		}
		div[data-testid="form-row"] {
			@media (min-width: 750px) {
				flex: 1 0 50%;
				max-width: 50%;
				padding-right: 0;
				padding-right: 30px;
				align-items: center;
			}
			@media (min-width: 320px) and (max-width: 749px) {
				flex: 1 0 100%;
				max-width: 100%;
				align-items: stretch;
			}
		}
		div[data-testid="form-row"] + div[data-testid="form-row"] {
			@media (min-width: 750px) {
				flex: 1 0 40%;
				max-width: 40%;
				padding-right: 30px;
				padding-left: 30px;
			}
			@media (min-width: 320px) and (max-width: 749px) {
				flex: 1 0 100%;
				max-width: 100%;
			}
		}
		div[data-testid="form-row"] + div[data-testid="form-row"] + div[data-testid="form-row"] {
			@media (min-width: 750px) {
				position: absolute;
				right: 23px;
				top: 50%;
				flex: 1 0 100%;
				max-width: inherit;
				padding-right: 0px;
				padding-left: 0px;
				margin-top: 16px;
			}
			@media (min-width: 320px) and (max-width: 749px) {
				flex: 1 0 100%;
				max-width: 100%;
			}
		}
		.ql-editor {
			font-size: 12px !important;
			line-height: 14px !important;
			font-family: GTAmerica !important;
			font-weight: 400 !important;
			text-align: left;
			p {
				text-align: left !important;
			}
			@media (min-width: 320px) and (max-width: 749px) {
				p {
					text-align: center;
				}
			}
			strong {
				display: block;
				text-transform: none;
				font-size: 23px;
				line-height: 30px;
				letter-spacing: 1.5px;
				font-weight: 500;
				padding-bottom: 15px;
				text-align: left;
				font-family: GTAmerica !important;
				@media (min-width: 320px) and (max-width: 749px) {
					text-align: center;
				}
			}
			span {
				display: block !important;
				font-family: GTAmerica !important;
				@media (min-width: 320px) and (max-width: 749px) {
					text-align: center;
				}
			}
		}
		input[type="email"] {
			border-radius: 30px !important;
			font-size: 12px !important;
			line-height: 20px !important;
			font-family: GTAmerica !important;
			padding: 6px 75px 4px 20px !important;
		}
		input[type="email"]::placeholder {
			color: #fff !important;
			opacity: 1 !important;
			font-size: 12px;
			line-height: 24px;
		}
		button[type="button"] {
			font-size: 12px !important;
			line-height: 20px !important;
			font-family: GTAmerica !important;
			font-weight: 400 !important;
			border: none !important;
			border-left: 1px solid #fff !important;
			padding: 4px 15px 5px 15px !important;
			margin-top: -26px;
			position: relative;
			top: -4px;
			right: 0px;
			border-radius: 0 30px 30px 0 !important;
			position: relative;
		}
	}
