/* template 404 */
.template-404 {
	font-weight: 100;
}
.template-404 .main-content {
	padding-top: 50px;
	padding-bottom: 50px;
	.h3 {
		font-size: 22px;
	}
	h1 + svg {
		@media (min-width: 320px) and (max-width: 749px) {
			width: 100%;
			height: auto;
			max-width: 352px;
		}
	}
	.empty-page-content {
		padding: 0px;
	}
	.desc {
		font-size: 16px;
		line-height: 24px;
		color: #212121;
		padding-top: 20px;
		padding-bottom: 60px;
		margin-bottom: 0px;
	}
	.info {
		letter-spacing: 0px;
		font-size: 16px;
		line-height: 140%;
		color: #212121;
		a {
			text-transform: none;
			color: #ba9163;
			text-decoration: none;
			// margin-left: 4px;
			font-size: 16px;
			line-height: 140%;
			letter-spacing: 0px;
			border-bottom: 1px solid transparent;
			position: relative;
			display: inline-block;
			margin-right: 30px;
			font-weight: 100;
			text-transform: lowercase;
			&:hover {
				border-color: #977045;
				color: #977045;
			}
			.icon-arrow-right {
				position: absolute;
				left: 100%;
				top: 2px;
				margin-left: 6px;
			}
		}
	}
	.search-form {
		max-width: 352px;
		margin: 0 auto;
		position: relative;
		input {
			padding: 11px 50px 13px 22px;
			max-width: 370px;
			font-size: 16px;
			font-weight: 100;
			line-height: 20px;
			color: #212121;
			margin: 0 auto 0;
			height: 45px;
			min-height: 45px;
			font-family: "GT America";
			outline: none;
			border-radius: 0px !important;
			border: 1px solid #b6b6b6;
			background: #fff;
		}
		.search-form-submit {
			position: absolute;
			right: 8px;
			top: 4px;
			width: 30px;
			height: 30px;
			color: #7a7a7a;
			background: none;
			border: none;
			padding: 0px;
			outline: none;
			&:hover {
				color: #212121;
			}
		}
	}
	input::placeholder {
		color: #212121 !important;
		opacity: 1 !important;
		font-size: 16px;
		line-height: 20px;
	}
}
/* template log in and sign in */
.template-customers-login .main-content,
.template-customers-register .main-content {
	padding-top: 50px;
	padding-bottom: 50px;
	@media (min-width: 320px) and (max-width: 749px) {
		padding-top: 40px;
		padding-bottom: 37px;
	}
	h1.h4 {
		// font-size: 22px;
		margin-bottom: 50px;
		text-transform: uppercase;
		@media (min-width: 320px) and (max-width: 749px) {
			margin-bottom: 30px;
		}
	}
	// .page-width {
	// 	@media (min-width: 320px) and (max-width: 749px) {
	//   		padding: 0px 30px!important;
	// 	}
	// }
	#LoginHeading {
		outline: none;
	}
	#customer_login,
	#RecoverPasswordForm,
	#RegisterForm {
		max-width: 370px;
		margin: 0 auto;
		.form-message__title,
		#RecoverHeading {
			outline: none;
		}
		#RecoverHeading {
			margin-bottom: 20px;
		}
		#RecoverHeading + p {
			margin-bottom: 50px;
		}
		.form-message--error li {
			/*color: #d20000;*/
			color: #212121;
			/*opacity: 0.5;*/
			opacity: 1;
		}
		#RecoverEmail-email-error,
		#RegisterForm-email-error,
		#RegisterForm-password-error {
			span {
				/*color: #d20000;*/
				color: #212121;
				/*opacity: 0.5;*/
				opacity: 1;
			}
		}
		label {
			display: none;
		}
		input:not([type="submit"]) {
			padding: 15px 22px;
			max-width: 370px;
			font-size: 16px;
			font-weight: 100;
			line-height: 20px;
			color: #212121;
			margin: 0 auto 15px;
			height: 50px;
			min-height: 50px;
			font-family: "GT America";
			outline: none;
			border-radius: 0px !important;
			border: 1px solid #b6b6b6;
			background: #fff;
		}
		.check-box {
			overflow: hidden;
			margin-bottom: 15px;
		}
		.radio_check_input {
			position: absolute;
			z-index: -1;
			opacity: 0;
		}
		.radio_check_input + label {
			display: inline-flex;
			align-items: center;
			user-select: none;
		}
		.radio_check_input + label::before {
			content: "";
			display: inline-block;
			width: 15px;
			height: 15px;
			min-height: 15px;
			flex-shrink: 0;
			flex-grow: 0;
			border: 1px solid #b6b6b6;
			border-radius: 0;
			margin-right: 10px;
			background-repeat: no-repeat;
			background-position: center center;
			background-size: 50% 50%;
			background: #fff;
		}
		.radio_check_input:checked + label::before {
			border-color: #0b76ef;
			background-color: #0b76ef;
			background-image: url("data:image/svg+xml,%3csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 8 8'%3e%3cpath fill='%23fff' d='M6.564.75l-3.59 3.612-1.538-1.55L0 4.26 2.974 7.25 8 2.193z'/%3e%3c/svg%3e");
		}
		.radio_check_lebel {
			width: auto;
			display: inline-block;
			overflow: hidden;
			text-transform: none;
			color: #212121;
			text-decoration: none;
			margin-left: 4px;
			font-size: 14px;
			line-height: 24px;
			letter-spacing: 1px;
			padding-left: 0;
			margin-left: 0px;
			border-bottom: 1px solid transparent;
			&:hover {
				color: #606060;
				//border-color: #606060;
			}
		}
		.btn2 {
			width: 100%;
			+ p {
				letter-spacing: 1px;
				font-size: 14px;
				font-weight: 100;
				line-height: 16px;
				color: #212121;

				a {
					text-transform: none;
					color: #ba9163;
					text-decoration: none;
					// margin-left: 4px;
					font-size: 14px;
					// line-height: 16px;
					letter-spacing: 0;
					// border-bottom: 1px solid transparent;
					// display: inline-block;
					// padding-bottom: 0px;
					font-weight: inherit;
					&:hover {
						border-color: #977045;
						color: #977045;
					}
				}
			}
		}
		// .btn {
		// 	color: #fff;
		// 	width: 100%;
		// 	display: block;
		// 	margin-bottom: 30px;
		// 	font-size: 14px;
		//     line-height: 20px;
		//     background: #212121;
		//     font-weight: 500;
		//     border:none;
		//     &:hover {
		//     	background: #606060;
		//     }
		// }
		.btn + p {
			letter-spacing: 1px;
			font-size: 14px;
			font-weight: 100;
			line-height: 16px;
			color: #212121;

			a {
				text-transform: none;
				color: #ba9163;
				text-decoration: none;
				margin-left: 4px;
				font-size: 14px;
				line-height: 16px;
				letter-spacing: 0px;
				border-bottom: 1px solid transparent;
				display: inline-block;
				padding-bottom: 0px;
				font-weight: 100;
				&:hover {
					border-color: #977045;
					color: #977045;
				}
			}
		}
		#HideRecoverPasswordLink {
			text-transform: none;
			color: #ba9163;
			text-decoration: none;
			margin-left: 4px;
			font-size: 14px;
			line-height: 24px;
			letter-spacing: 0px;
			border-bottom: 1px solid transparent;
			&:hover {
				border-color: #977045;
				color: #977045;
			}
		}

		#RecoverPassword {
			display: inline-block;
			margin-bottom: 18px;
			margin-top: -2px;
			text-transform: none;
			text-decoration: none;
			letter-spacing: 0px;
			font-size: 14px;
			font-weight: 100;
			line-height: 20px;
			color: #212121;
			border-bottom: 1px solid transparent;
			&:hover {
				color: #606060;
				border-color: #606060;
			}
		}
	}
	input::placeholder {
		color: #212121 !important;
		opacity: 1 !important;
		font-size: 16px;
		line-height: 20px;
	}
}

