/* template contact-info */
#contact-info {
	background: #fff;
	//padding-top: 25px;
	// padding-bottom: 50px;
	// @media (min-width: 320px) and (max-width: 749px) {
	//padding-top: 20px;
	// padding-bottom: 40px;
	// }
	.page-width2 {
		// max-width: 1260px;
		// padding: 0px 50px!important;
		// @media (min-width: 320px) and (max-width: 375px) {
		// 		padding: 0px 20px !important;
		// }
		// @media (min-width: 375px) and (max-width: 749px) {
		// 	padding: 0px 30px!important;
		// }
		.grids {
			overflow: hidden;
			.coll-6 {
				float: left;
				width: 580px;
				@media (min-width: 992px) and (max-width: 1199px) {
					width: 380px;
				}
				@media (min-width: 768px) and (max-width: 991px) {
					width: 280px;
				}
				@media (min-width: 320px) and (max-width: 767px) {
					width: 100%;
					float: none;
					// margin-bottom: 30px;

					.contact-img-wrapper {
						padding-bottom: 30px;
					}
				}
				img {
					width: 100%;
				}
			}
			.coll-6 + .coll-6 {
				float: none;
				width: auto;
				overflow: hidden;
				.item_info {
					margin: -4px 0 0;
					padding-left: 50px;
					padding-right: 60px;
					@media (min-width: 992px) and (max-width: 1199px) {
						padding-left: 30px;
						padding-right: 0;
					}
					@media (min-width: 768px) and (max-width: 991px) {
						padding-left: 30px;
						padding-right: 0;
					}
					@media (min-width: 320px) and (max-width: 767px) {
						margin: 0;
						padding-left: 0;
						padding-right: 0;
					}
					.item_desc p,
					.item_desc2 p {
						font-size: 16px;
						font-weight: 100;
						line-height: 140%;
						color: #212121;
						padding-bottom: 30px;
					}
					.item_phone {
						padding-bottom: 30px;
						li {
							line-height: 26px;
							@media screen and (max-width: 767px) {
								padding: 10px 0 0;
							}
							&:first-child{
								@media screen and (max-width: 767px) {
									padding: 0;
								}
							}
							a {
								font-size: 16px;
								line-height: 20px;
								letter-spacing: 1px;
								color: #212121;
								font-weight: 500;
								border-bottom: 1px solid transparent;
								text-decoration: none;
								display: inline-block;
								&:hover {
									color: #606060;
									border-color: #606060;
								}
							}
						}
					}
					.item_email {
						padding-bottom: 30px;
						@media (min-width: 320px) and (max-width: 1199px) {
							padding-bottom: 10px;
						}
						li {
							font-weight: 500;
							font-size: 16px;
							line-height: 26px;
							// display: inline-block;
							color: #212121;
							letter-spacing: 1px;
							a {
								font-size: 16px;
								line-height: 20px;
								color: #212121;
								font-weight: 100;
								border-bottom: 1px solid transparent;
								text-decoration: none;
								display: inline-block;
								text-transform: none;
								letter-spacing: 0px;
								&:hover {
									color: #606060;
									border-color: #606060;
								}
							}
						}
					}
					.section-button-holder {
						.btn2 {
							min-width: 220px;
							@media (min-width: 992px) and (max-width: 1199px) {
								min-width: 200px;
							}
						}
					}
					.btn-group {
						.journeybtn {
							font: 500 14px/24px "GT America", sans-serif;
							text-align: center;
							letter-spacing: 1.5px;
							text-transform: uppercase;
							color: #212121;
							background: #fff;
							border: 1px solid #212121;
							border-radius: 33px;
							padding: 13px 18px 11px;
							float: left;
							margin-top: 22px;
							margin-right: 22px;
							min-width: 220px;
							@media (min-width: 992px) and (max-width: 1199px) {
								min-width: 200px;
							}
							&:hover {
								background: #e1e1e1;
							}
						}
						.journeybtn + .journeybtn {
							margin-right: 0px;
						}
					}
				}
			}
		}
	}
}
