	<script src="https://code.jquery.com/jquery-3.5.1.min.js"
		integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
	<script>
        function hideAnnouncementBar(){
          $.cookie('is_announcement_bar_closed', 'true', { expires: 2, path:'/' })
          $(".announcement-bar").css("display", "none");
          $("body").removeClass("has-announcement-bar");
        }

        function checkCookieAnnouncementBar(){
          let isAnnouncementBarClosed = $.cookie('is_announcement_bar_closed')
          if(isAnnouncementBarClosed == 'true') {
            hideAnnouncementBar();
          }
          else {
          	if ($(".announcement-bar").length) {
				$(".announcement-bar").css("display", "block");
				$("body").addClass("has-announcement-bar");
			}
          }
        }
      
		$(document).ready(function () {       
			$(".js-mobile-nav-toggle").click(function () {
				$(".announcement-bar").css("display", "none");
				$("body").removeClass("has-announcement-bar");
				$('body').toggleClass('lock-scroll');
			});
			$(document).click(function () {
				var topheader = $('.mobile-nav-wrapper').hasClass('js-menu--is-open');
				if (topheader == false) {
					if ($(".announcement-bar").length) {
						$(".announcement-bar").css("display", "block");
						$("body").addClass("has-announcement-bar");
						$('body').removeClass('lock-scroll');
						checkCookieAnnouncementBar();
					}
				}
              
			});
			$(".site-header__menu-toggle--open").click(function () {
				$(".announcement-bar").addClass("bars");
				$('body').addClass('desk-lock-scroll');
			});
			$(document).click(function () {
				var topheader = $('.site-header__menu-toggle--open').hasClass('actives');
				if (topheader == false) {
					$(".announcement-bar").removeClass("bars");
					$('body').removeClass('desk-lock-scroll');
				}
			});
                    
    		checkCookieAnnouncementBar();
		});
	</script>
