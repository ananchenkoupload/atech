{% if section.settings.message %}
	<style>
		{% if section.settings.logo != blank %}
			.site-header__logo-image {
				max-width: {{ section.settings.logo_max_width | escape }}px;
			}
		{% endif %}

		{% if section.settings.align_logo=='left'%}
			.site-header__logo-image {
				margin: 0;
			}
		{% endif %}
	</style>
{% endif %}

{% include 'search-drawer' %}

<div data-section-id="{{ section.id }}" data-section-type="header-section" data-header-section id="header">
	{% if section.settings.message %}
		{% if section.settings.home_page_only == false or request.page_type == 'index' %}
			<style>
				.announcement-bar {
					background-color: {{ section.settings.color_bg }};
				}
                            
                .announcement-bar__container {
					padding: 11px 0 12px;
				}      
              
                .announcement-bar__link {
				    text-decoration-line: none;
				}

				.announcement-bar__container a {
					color: {{ section.settings.color_text }};
					text-decoration: none;
				}
				@media (max-width: 767px) {
					.announcement-bar__container p a:last-child {
						display: block;
					}

					.announcement-bar__container a {
						font-size: 10px;
						line-height: 12px;
					}
				}
				.announcement-bar__container a:hover {
					color: #BA9163;
				}
				.announcement-bar__container p {
					margin-bottom: 0;
					color: {{ section.settings.color_text }};
				}
				.announcement-bar__link:hover {   
                 
                  {% comment %}
                  {% assign brightness=section.settings.color_bg | color_brightness %}
                  {% if brightness <=192 %}
                  {% assign lightenAmount=255 | minus: brightness | divided_by: 255 | times: 16 %}
                  background-color: {{ section.settings.color_bg | color_lighten: lightenAmount }};
                  {% else %}
                  {% assign darkenAmount=255 | divided_by: brightness | times: 8 %}
                  background-color: {{ section.settings.color_bg | color_darken: darkenAmount }};
                  {% endif %}
                  {% endcomment %}
				}

              	.announcement-bar__link:hover .announcement-bar__message {   
                  color: var(--color-text-focus);
                  text-decoration-line: underline;
				}
              
				.announcement-bar__message {
                  	display: inline;
					color: {{ section.settings.color_text }};
				}
              
                .announcement-bar__close-btn {
                  position: absolute;
                  top: 6px;
                  right: 6px;
                  background-color: transparent;
                  border-color: transparent;
                  color: #f6f6f6;
                }
              
              	@media only screen and (max-width: 767px) {
                    .announcement-bar__close-btn {
                      top: 8px;
                      right: 5px;
                    }
                }
              
			</style>
			<div class="announcement-bar" role="region" aria-label="{{ 'sections.header.announcement_bar_label' | t }}" style="display: none;">
				<div class="announcement-bar__container">

					<div class="announcement-bar__link">
						<div class="announcement-bar__message">{{ section.settings.message_text }}</div>
					</div>

					<button type="button" class="announcement-bar__close-btn" onclick="hideAnnouncementBar()" class="close" aria-label="Close">
						<span aria-hidden="true">×</span>
					</button>
				</div>
			</div>
		{% endif %}
	{% endif %}

	<header class="site-header" role="banner">
		<div class="page-width2 page-width2--fluid header-content site-header__mobile-nav">
			<div>
				<span class="btn2 btn2-link site-header__menu-toggle--opener">
					{% include 'icon-hamburger-new' %}
					{% include 'icon-close-new' %}
				</span>
				<script>
					$(document).ready(function () {
						$('.site-header__menu-toggle--opener').click(function () {
							$('.grid__item.medium-up--one-half.small--hide').toggleClass("active");
							$(this).toggleClass("active");
							$(".site-nav__childlist-item a").addClass('parentcategory');
						});
					});
				</script>
				<span class="btn2 btn2-link site-header__menu-toggle--opener mobile js-mobile-nav-toggle mobile-nav--open">
					{% include 'icon-hamburger-new' %}
					{% include 'icon-close-new' %}
				</span>
			</div>
			<div class="logo-holder {% if section.settings.enable_svg %} svg-logo {% endif %}">
				{% comment %}
					Use the uploaded logo from theme settings if enabled.
					Site name gets precedence with H1 tag on homepage, div on other pages.
				{% endcomment %}
				{% if request.page_type == 'index' %}
					<h1 class="h2 site-header__logo">
				{% else %}
					<div class="h2 site-header__logo">
				{% endif %}

				{% if section.settings.logo %}
					{% comment %}
					{%- assign img_url = "Aromatech-Header-logo.svg" | asset_url -%}
					{% endcomment %}
					<a href="{{ routes.root_url }}"
						class="site-header__logo-image{% if section.settings.align_logo == 'center' %} site-header__logo-image--centered{% endif %}"
						data-image-loading-animation>
						{% capture logo_alt %}{{ section.settings.logo.alt | default: shop.name }}{% endcapture %}
						{% comment %}
						SVG format must be loaded from Settings/Files
						{% endcomment %}
						{% if section.settings.enable_svg %}
							{{ section.settings.html }}
						{% else %}
							<img class="lazyload js" src="{{ section.settings.logo | img_url: '592x592' | replace: '_592x592.png', '.svg' }}" data-src="{{ img_url }}"
								data-widths="[180, 360, 540, 720, 900, 1080, 1296, 1512, 1728, 2048]"
								data-aspectratio="{{ section.settings.logo.aspect_ratio }}" data-sizes="auto"
								alt="{{ logo_alt | escape }}">
							<noscript>
								{% capture image_size %}{{ section.settings.logo_max_width | escape }}x{% endcapture %}
								<img src="{{ section.settings.logo | img_url: image_size }}"
									srcset="{{ section.settings.logo | img_url: image_size }} 1x, {{ section.settings.logo | img_url: image_size, scale: 2 }} 2x"
									alt="{{ section.settings.logo.alt | default: shop.name }}"
									style="max-width: {{ section.settings.logo_max_width }}px;">
							</noscript>
						{% endif %}
					</a>
				{% else %}
						
					<a class="site-header__logo-link" href="{{ routes.root_url }}">
						{% if section.settings.enable_svg %}
							{{ section.settings.html }}
						{% else %}
							{{ shop.name }}
						{% endif %}
					</a>
				{% endif %}
				

				{% if request.page_type == 'index' %}
					</h1>
				{% else %}
					</div>
				{% endif %}
			</div>
			<div>
				<div class="site-header__icons-wrapper">
					<div class="site-header__icons-item currency-switcher">
						{% include 'multi-currency' %}
					</div>
					<div class="site-header__icons-item">
						<button type="button" class="btn2 btn2-link site-header__icon site-header__search-toggle js-drawer-open-top"
							data-predictive-search-open-drawer>
							{% include 'icon-search-new' %}
							<span class="icon__fallback-text">{{ 'layout.navigation.search' | t }}</span>
						</button>
					</div>
					<div class="site-header__icons-item">
						{% if shop.customer_accounts_enabled %}
							{% if customer %}
								<a href="{{ routes.account_url }}" class="btn2 btn2-link site-header__icon site-header__account">
									{% include 'icon-login-new' %}
									<span class="icon__fallback-text">{{ 'layout.customer.account' | t }}</span>
								</a>
							{% else %}
								<a href="{{ routes.account_login_url }}" class="btn2 btn2-link site-header__icon site-header__account">
									{% include 'icon-login-new' %}
									<span class="icon__fallback-text">{{ 'layout.customer.log_in' | t }}</span>
								</a>
							{% endif %}
						{% endif %}

					</div>
					<div class="site-header__icons-item">
						<a href="{{ routes.cart_url }}" class="btn2 btn2-link site-header__icon site-header__cart js-ajax-cart-drawer-trigger">
							{% include 'зм' %}
							<span class="icon__fallback-text">{{ 'layout.cart.title' | t }}</span>
							<div id="CartCount"
								class="site-header__cart-count{% if cart.item_count == 0 %} _hide{% endif %} critical-hidden">
								<span class="js-ajax-cart-counter-header">{{ cart.item_count }}</span>
								<span class="icon__fallback-text medium-up--hide">{{ 'layout.cart.items_count' | t: count: cart.item_count
									}}</span>
							</div>
						</a>
					</div>
				</div>
			</div>
		</div>
		{% if section.settings.align_logo == 'left' %}

				<nav class="grid__item medium-up--one-half small--hide" id="AccessibleNav" role="navigation">
					{% include 'site-nav', linklist: section.settings.main_linklist %}
				</nav>
			{% include 'site-nav-new', linklist: section.settings.main_linklist %}

		{% endif %}

		<nav class="mobile-nav-wrapper critical-hidden" role="navigation">
			<ul id="MobileNav" class="mobile-nav">
				{% for link in linklists[section.settings.main_linklist].links %}
					{%- assign outerLoopIndex = forloop.index -%}
					<li class="mobile-nav__item{% unless forloop.last %} border-bottom{% endunless %}">
						{% if link.links != blank %}
							{% capture child_list_handle %}{{ link.handle }}-{{ outerLoopIndex }}{% endcapture %}
							<button type="button"
								class="btn--link js-toggle-submenu mobile-nav__link{% if link.child_active %} mobile-nav__link--active{% endif %}"
								data-target="{{ child_list_handle }}" data-level="1" aria-expanded="false">
								<span class="mobile-nav__label">{{ link.title | escape }}</span>
								<div class="mobile-nav__icon">
									{% include 'icon-chevron-right' %}
								</div>
							</button>
							<ul class="mobile-nav__dropdown" data-parent="{{ child_list_handle }}" data-level="2">
								<li class="visually-hidden" tabindex="-1" data-menu-title="2">
									{{- link.title }} {{ 'layout.navigation.menu' | t -}}
								</li>
								<li class="mobile-nav__item border-bottom">
									<div class="mobile-nav__table">
										<div class="mobile-nav__table-cell mobile-nav__return">
											<button class="btn--link js-toggle-submenu mobile-nav__return-btn" type="button" aria-expanded="true"
												aria-label="{{ link.title }}">
												{% include 'icon-arrow-left-new' %}
											</button>
										</div>
										<span
											class="mobile-nav__sublist-link mobile-nav__sublist-header mobile-nav__sublist-header--main-nav-parent">
											<span class="mobile-nav__label">{{ link.title | escape }}</span>
										</span>
									</div>
								</li>

								{% assign current_tab = '' %}

								{% for childlink in link.links %}

									{% if current_tab == blank %}
										{% assign current_tab = childlink.url %}
									{% endif %}

								<li class="mobile-nav__item{% unless forloop.last %} border-bottom{% endunless %}">
									{% if childlink.links != blank %}
									{% capture grandchild_list_handle %}{{ childlink.handle }}-{{ outerLoopIndex }}-{{ forloop.index }}{%
									endcapture %}
									<button type="button" class="btn--link js-toggle-submenu mobile-nav__link mobile-nav__sublist-link"
										data-target="{{ grandchild_list_handle }}" aria-expanded="false">
										<span class="mobile-nav__label">{{ childlink.title | escape }}</span>
										<div class="mobile-nav__icon">
											{% include 'icon-chevron-right' %}
										</div>
									</button>
									<ul class="mobile-nav__dropdown" data-parent="{{ grandchild_list_handle }}" data-level="3">
										<li class="visually-hidden" tabindex="-1" data-menu-title="3">
											{{- childlink.title }} {{ 'layout.navigation.menu' | t -}}
										</li>
										<li class="mobile-nav__item border-bottom">
											<div class="mobile-nav__table">
												<div class="mobile-nav__table-cell mobile-nav__return">
													<button type="button" class="btn--link js-toggle-submenu mobile-nav__return-btn"
														data-target="{{ child_list_handle }}" aria-expanded="true" aria-label="{{ childlink.title }}">
														{% include 'icon-arrow-left-new' %}
													</button>
												</div>
												<a href="{{ childlink.url }}" class="mobile-nav__sublist-link mobile-nav__sublist-header" {% if
													childlink.current %} aria-current="page" {% endif %}>
													<span class="mobile-nav__label">{{ childlink.title | escape }}</span>
												</a>
											</div>
										</li>
										{% for grandchildlink in childlink.links %}
										<li class="mobile-nav__item{% unless forloop.last %} border-bottom{% endunless %}">
											<a href="{{ grandchildlink.url }}" class="mobile-nav__sublist-link" {% if grandchildlink.current %}
												aria-current="page" {% endif %}>
												<span class="mobile-nav__label">{{ grandchildlink.title | escape }}</span>
											</a>
										</li>
										{% endfor %}
									</ul>
									{% else %}
									<a href="{{ childlink.url }}" class="mobile-nav__sublist-link" {% if childlink.current %}
										aria-current="page" {% endif %}>
										<span class="mobile-nav__label">{{ childlink.title | escape }}</span>
									</a>
									{% endif %}
								</li>
								{% endfor %}


								<script>console.log('current', {{ current_tab | json }})</script>

								{% assign show_carousel = false %}

								{% for collection in collections %}
									{% if collection.url == current_tab %}
										{% assign show_carousel = true %}
									{% endif %}
								{% endfor %}

								{% if show_carousel %}
									<div class="menu-carousel">
										<div class="menu-carousel__title">
											BESTSELLERS
										</div>
										<div class="menu-carousel__wrap">

											<div class="menu-carousel__content">

												{% for collection in collections %}
													{% if collection.url == current_tab %}
														{% assign products_sort = collection.products | sort: 'title' %}
														{% for product in products_sort %}
															{% if product.tags contains 'Bestseller' %}
																<div class="menu-carousel__item">
																	<a href="{{ product.url | within: collection }}">
																		<div class="menu-carousel__item-image">
																			<img src="{{ product.featured_media.preview_image | img_url: '480x' }}" alt="">
																		</div>
																		<div class="menu-carousel__item-title">{{ product.title }}</div>
																		<div class="menu-carousel__item-price">From {% include 'product-price-listing', product: product, show_vendor: show_vendor, collectionPage: collectionPage %}</div>
																	</a>
																</div>
															{% endif %}
														{% endfor %}
													{% endif %}
												{% endfor %}


											</div>
										</div>
									</div>
								{% endif %}





							</ul>


						{% else %}
							<a href="{{ link.url }}" class="mobile-nav__link{% if link.active %} mobile-nav__link--active{% endif %}" {%
								if link.current %} aria-current="page" {% endif %}>
								<span class="mobile-nav__label">{{ link.title | escape }}</span>
							</a>
						{% endif %}
					</li>
				{% endfor %}

				<div class="mobile-switcher">
					{% include 'multi-currency' %}
				</div>

			</ul>



			<div class="mob-links">
				<div class="mob-login"><a href="/account/login">log in</a></div>
				<div class="mob-search">
					<div class="txt-search">

					</div>

					<form action="/search" method="get" class="search-bar" role="search">
						{% comment %}<input type="hidden" name="type" value="product">{% endcomment %}
						<button type="submit" class="search-bar__submit">

							<span class="search-nav-text">{{ 'general.search.submit' | t }}</span>
						</button>
						<input type="search" name="q" class="search-bar__input" value="{{ search.terms | escape }}"
							placeholder="{{ 'general.search.placeholder' | t }}" aria-label="{{ 'general.search.placeholder' | t }}">
					</form>

					<!--<form class="search" action="/search">
						<input
							type="text"
							placeholder="Search"
							name="q"
							value="{{ search.terms | escape }}"
						/>
						<input type="submit" value="Search" />
					</form>-->
				</div>
			</div>
		</nav>
	</header>
